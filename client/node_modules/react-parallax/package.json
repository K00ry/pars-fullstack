{
    "name": "react-parallax",
    "author": "Richard Rutsche",
    "version": "1.8.0",
    "description": "A React Component for parallax effect working in client-side and server-side rendering environment",
    "repository": {
        "type": "git",
        "url": "https://github.com/RRutsche/react-parallax.git"
    },
    "main": "dist/index.js",
    "scripts": {
        "test": "jest",
        "test:watch": "npm run test -- --watch",
        "test:cover": "npm run test -- --coverage",
        "dev": "NODE_ENV=development && webpack-dev-server --progress --config webpack.config.dev.js",
        "dist": "NODE_ENV=production && webpack -p --progress --config webpack.config.build.js",
        "lint": "eslint src",
        "format": "prettier --write \"*/**/*.{js,json,css,scss}\" \"*.js\"",
        "precommit": "lint-staged && git diff --exit-code"
    },
    "keywords": [
        "react",
        "reactjs",
        "parallax",
        "scroll",
        "jsx"
    ],
    "peerDependencies": {
        "prop-types": "^15.5.8",
        "react": "^0.14.9 || ^15.3.0-0 || ^16.0.0",
        "react-dom": "^0.14.9 || ^15.3.0-0 || ^16.0.0"
    },
    "license": "MIT",
    "devDependencies": {
        "autoprefixer-loader": "^3.2.0",
        "babel-core": "^6.24.1",
        "babel-eslint": "^7.2.3",
        "babel-jest": "^21.2.0",
        "babel-loader": "^7.0.0",
        "babel-plugin-react-transform": "^2.0.2",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-runtime": "^6.23.0",
        "css-loader": "^0.28.1",
        "eslint": "^4.7.2",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-destructuring": "^2.1.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-react": "^7.1.0",
        "extract-text-webpack-plugin": "^2.1.0",
        "file-loader": "^0.11.1",
        "fsevents": "^1.1.3",
        "html-webpack-plugin": "^2.28.0",
        "husky": "^0.14.3",
        "imports-loader": "^0.7.1",
        "jasmine-core": "^2.4.1",
        "jest": "^21.2.1",
        "jscs": "^3.0.7",
        "jsdom": "^11.3.0",
        "json-loader": "^0.5.3",
        "karma": "~0.13.11",
        "karma-chrome-launcher": "~0.2.1",
        "karma-jasmine": "~0.3.6",
        "lint-staged": "^4.3.0",
        "node-libs-browser": "^2.0.0",
        "node-sass": "^4.5.2",
        "prettier": "^1.7.4",
        "prop-types": "^15.6.0",
        "react": "^16.0.0",
        "react-addons-test-utils": "^15.6.2",
        "react-dom": "^16.0.0",
        "react-hot-loader": "^1.3.1",
        "react-router": "^3.0.0",
        "react-transform-hmr": "^1.0.4",
        "sass-loader": "^6.0.3",
        "script-loader": "^0.7.0",
        "style-loader": "^0.13.0",
        "url-loader": "^0.5.6",
        "webpack": "^2.5.1",
        "webpack-dev-server": "^2.4.5",
        "webpack-merge": "^4.1.0"
    },
    "lint-staged": {
        "gitDir": "./",
        "linters": {
            "src/**/*.js": [
                "prettier --write",
                "eslint",
                "git add"
            ],
            "*.{json,css,scss}": [
                "prettier --write",
                "git add"
            ]
        }
    },
    "jest": {
        "setupFiles": [
            "<rootDir>/setupFile.js"
        ]
    }
}
